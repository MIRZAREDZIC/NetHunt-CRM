    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "critters": "^0.0.25",
    "cypress": "^14.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "node-mocks-http": "^1.17.2",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
