{
  "name": "crm-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "sudo rm -rf .next || true",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "ANALYZE=true npm run build",
    "bundle-analyzer": "npx webpack-bundle-analyzer .next/static/chunks/*.js",
    "type-check": "tsc --noEmit",
    "dev:analyze": "ANALYZE=true npm run dev",
    "production-check": "node production-check.js",
    "quick-check": "node quick-check.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^5.0.1",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "@sentry/nextjs": "^9.25.1",
    "@tanstack/react-query": "^5.79.0",
    "@tanstack/react-query-devtools": "^5.79.0",
    "axios": "^1.9.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.14.0",
    "next": "^14.2.29",
    "next-pwa": "^5.6.0",
    "notistack": "^3.0.2",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.57.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@next/bundle-analyzer": "^15.3.3",
